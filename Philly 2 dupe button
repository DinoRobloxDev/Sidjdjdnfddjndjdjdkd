-- Services
local Players = game:GetService("Players")
local Workspace = game:GetService("Workspace")
local RunService = game:GetService("RunService")

-- Variables
local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")

-- Store original player position
local oldPlayerPos = HumanoidRootPart.Position

-- Find the "prompt" object in a more reliable way
local washMachine = Workspace:WaitForChild("WashingMachine")
local prompt = nil
if washMachine then
    -- Find the ProximityPrompt anywhere under the WashingMachine
    prompt = washMachine:FindFirstChildWhichIsA("ProximityPrompt", true)
end

if not prompt then
    warn("ProximityPrompt not found. The object might have a different hierarchy or name.")
else
    -- SETTING PROXIMITYPROMPT PROPERTIES
    prompt.Enabled = true
    prompt.HoldDuration = 0
    prompt.MaxActivationDistance = 12
    prompt.Exclusivity = Enum.ProximityPromptExclusivity.AlwaysShow
end

-- UI Creation
local playerGui = LocalPlayer:WaitForChild("PlayerGui")
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "AutoMoveUI"
screenGui.Parent = playerGui

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 200, 0, 80)
frame.Position = UDim2.new(0.5, -100, 0.5, -40)
frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
frame.BorderColor3 = Color3.fromRGB(20, 20, 20)
frame.Draggable = true
frame.Parent = screenGui

local title = Instance.new("TextLabel")
title.Text = "Super Speed"
title.Size = UDim2.new(1, 0, 0, 25)
title.Position = UDim2.new(0, 0, 0, 0)
title.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
title.TextColor3 = Color3.new(1, 1, 1)
title.Parent = frame

local toggleButton = Instance.new("TextButton")
toggleButton.Name = "ToggleButton"
toggleButton.Text = "START"
toggleButton.Size = UDim2.new(0.9, 0, 0, 40)
toggleButton.Position = UDim2.new(0.05, 0, 0.4, 0)
toggleButton.BackgroundColor3 = Color3.fromRGB(50, 150, 50)
toggleButton.TextColor3 = Color3.new(1, 1, 1)
toggleButton.Parent = frame

-- Scripting the logic
toggleButton.MouseButton1Click:Connect(function()
    -- Get original position before teleporting
    oldPlayerPos = HumanoidRootPart.Position
    
    -- Teleport to the washing machine instantly
    HumanoidRootPart.CFrame = CFrame.new(washMachine.Position)
    
    toggleButton.Text = "WORKING..."
    
    if prompt then
        -- This for loop will instantly spam the functions 700 times
        for i = 1, 700 do
            prompt:InputHoldBegin()
            prompt:InputHoldEnd()
        end
    end
    
    -- Teleport back to the original position
    HumanoidRootPart.CFrame = CFrame.new(oldPlayerPos)
    
    toggleButton.Text = "START"
end)

print("Super Speed script loaded successfully.")
